n = int (input ("Enter the digit from 0 to 9:"))
print ("Entered digit is :", end ='')
if n ==0:
    print ("Zero")
elif n==1 :
    print ("One")
elif n==2 :
    print ("Two")
elif n==3 :
    print ("Three")
elif n==4 :
    print ("Four")
elif n==5 :
    print ("Five")
elif n==6 :
    print ("Six")
elif n==7 :
    print ("Seven")
elif n==8 :
    print ("Eight")
elif n==9 :
    print ("nine")
else :
    print ("Not a digit!")


